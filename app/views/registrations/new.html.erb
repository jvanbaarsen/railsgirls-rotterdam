<div class="panel panel-default">
  <%= bootstrap_form_tag url: "https://formbox.es/4NT3MfwE6kwF5MB96iY5uzW7" do |f| %>
    <div class="panel-body">
      <%= f.text_field :name %>
      <%= f.text_field :age %>
      <%= f.text_field :gender %>
      <%= f.email_field :email %>
      <%= f.text_field :phone_number %>
      <%= f.text_field :education_level %>
      <%= f.text_field :area_of_study %>
      <%= f.text_field :coding_experience %>
      <%= f.text_field :special_diet, label: "Special dietary requirments or allergies?" %>
      <%= f.text_area :motivation %>
    </div>
    <div class="panel-footer">
      <%= f.submit "Register" %>
    </div>
  <% end %>
</div>
